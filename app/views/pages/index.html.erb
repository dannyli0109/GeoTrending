<ul class="trend-list">

</ul>


<script>

$.ajax({
  url: '/api/trends/1'
}).done(function(res){
  $(".trend-list").html("")
  res.forEach(function(element) {
    $(".trend-list").append($("<li>" + '<a href="/pages/'+ element["name"] + '">' + element["name"] + "</a>" + "</li>"))
  })
})



</script>
